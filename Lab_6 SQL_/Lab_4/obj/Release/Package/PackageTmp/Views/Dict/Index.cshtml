@using Lab_4.Models
@{
    Layout = null;
    List<Telephone> telephones = ViewBag.Select;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
        <h2>Dictionary of telephones</h2>
        <table border="1">
            <tr>
                <th>Id</th>
                <th>Surnames</th>
                <th>Telephone Numbers</th>
            </tr>
            @foreach (Telephone telephone in telephones)
            {
                <tr>
                    <td>@telephone.Id</td>
                    <td>@telephone.Surname</td>
                    <td>@telephone.Number</td>
                </tr>
            }
        </table>
        <h3>@Html.ActionLink("Add", "Add")</h3>
        <h3>@Html.ActionLink("Update", "Update")</h3>
        <h3>@Html.ActionLink("Delete", "Delete")</h3>
    </div>
    <footer>Create with Entity Framework</footer>
</body>
</html>
